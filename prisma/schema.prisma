generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  name     String
  username String   @unique
  password String
  gender   String
  created  DateTime @default(now())
  updated  DateTime @updatedAt
}

